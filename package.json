{
  "name": "vectoricons-portfolio",
  "version": "1.0.0",
  "description": "VectorIcons - Icon Marketplace Backend Architecture (Portfolio)",
  "author": "Scott Russell",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/vectoricons-portfolio.git"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "test": "jest --noStackTrace --runInBand --verbose --detectOpenHandles --forceExit",
    "test:units": "jest --selectProjects units --verbose --noStackTrace --runInBand",
    "test:integration": "jest --selectProjects integration --verbose --noStackTrace --runInBand",
    "test:coverage": "jest --coverage --noStackTrace --runInBand",
    "test:watch": "jest --watch",
    "docs:generate": "documentation build 'src/common/base/**/*.js' 'src/common/cursor/**/*.js' 'src/common/mixins/repository/**/*.js' 'src/common/cache/**/*Service.js' 'src/common/cache/**/*Adapter.js' 'src/common/event-bus/EventBus.js' 'src/common/access-control/**/*Service.js' 'src/aws/**/*Service.js' 'src/products/**/*Entity.js' 'src/products/**/*Repository.js' 'src/products/**/*Service.js' 'src/images/**/*Entity.js' 'src/images/**/*Repository.js' 'src/images/**/*Service.js' 'src/carts/**/*Entity.js' 'src/carts/**/*Repository.js' 'src/carts/**/*Service.js' 'src/orders/**/*Entity.js' 'src/orders/**/*Repository.js' 'src/orders/**/*Service.js' 'src/transactions/**/*Entity.js' 'src/transactions/**/*Repository.js' 'src/transactions/**/*Service.js' 'src/downloads/**/*Entity.js' 'src/downloads/**/*Repository.js' 'src/downloads/**/*Service.js' 'src/favorites/**/*Entity.js' 'src/favorites/**/*Repository.js' 'src/favorites/**/*Service.js' '!src/**/__tests__/**' '!src/**/*.test.js' '!src/**/*.bak.js' '!src/**/index.js' '!src/**/seed.js' '!src/**/fixtures.js' -f html -o docs --shallow --project-name 'VectorIcons Backend' --project-version 1.0.0",
    "docs:serve": "documentation serve 'src/common/access-control/**/*.js' 'src/common/cache/**/*.js' 'src/common/event-bus/**/*.js' --watch"
  },
  "dependencies": {
    "@fastify/env": "^5.0.2",
    "aws-sdk": "^2.1691.0",
    "bcryptjs": "^3.0.2",
    "fs-extra": "^11.3.0",
    "handlebars": "^4.7.8",
    "inflected": "^2.1.0",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "mitt": "^3.0.1",
    "node-cache": "^5.1.2",
    "nodemailer": "^7.0.5",
    "objection": "^3.1.5",
    "pg": "^8.13.1",
    "validatorjs": "^3.22.1",
    "winston": "^3.17.0",
    "yaml": "^2.8.1"
  },
  "devDependencies": {
    "aws-sdk-mock": "^6.2.1",
    "documentation": "^14.0.0",
    "dotenv": "^16.4.7",
    "dotenv-cli": "^9.0.0",
    "jest": "^29.7.0",
    "supertest": "^7.1.4"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/__tests__/**",
      "!src/**/*.test.js",
      "!src/**/*.unit.test.js"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.js",
      "**/__tests__/**/*.unit.test.js"
    ]
  },
  "keywords": [
    "nodejs",
    "backend",
    "api",
    "postgresql",
    "redis",
    "event-driven",
    "soa",
    "service-oriented-architecture",
    "objection-orm",
    "fastify",
    "aws",
    "testing",
    "portfolio"
  ],
  "notes": {
    "portfolio": "This is a portfolio showcase. Several dependencies have been modified or removed:",
    "removed": [
      "@vectoricons.net/db - Proprietary database package replaced with standard knex/pg",
      "@openai/codex - Internal AI tooling removed",
      "Deployment and infrastructure scripts removed"
    ],
    "not_runnable": "This code is not runnable without the full infrastructure setup (database, Redis, AWS credentials, etc.). See README.md for details."
  }
}
